<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="icones\accueil.ico" />
    <None Remove="icones\client.ico" />
    <None Remove="icones\commande.ico" />
    <None Remove="icones\connection.ico" />
    <None Remove="icones\fournisseur.ico" />
    <None Remove="icones\icone1.ico" />
    <None Remove="icones\inscription.ico" />
    <None Remove="icones\proprietaire.ico" />
    <None Remove="icones\vendeur.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="33.0.1" />
    <PackageReference Include="MahApps.Metro" Version="2.4.10" />
    <PackageReference Include="MahApps.Metro.IconPacks" Version="5.1.0" />
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="9.0.4" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Design" Version="9.0.4">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.EntityFrameworkCore.SqlServer" Version="9.0.4" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="9.0.4">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="BouquetLibrary">
      <HintPath>bin\Debug\net8.0-windows\BouquetLibrary.dll</HintPath>
    </Reference>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="icones\accueil.ico" />
    <Resource Include="icones\client.ico" />
    <Resource Include="icones\connection.ico" />
    <Resource Include="icones\fournisseur.ico" />
    <Resource Include="icones\inscription.ico" />
    <Resource Include="icones\proprietaire.ico" />
    <Resource Include="icones\vendeur.ico" />
  </ItemGroup>

</Project>
